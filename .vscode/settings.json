{
    "editor.semanticHighlighting.enabled": true,
    "python.defaultInterpreterPath": "C:\\Users\\kamim\\code\\Tennis-Analyzer\\.venv\\Scripts\\python.exe",
    "editor.semanticTokenColorCustomizations": {
        "[One Dark Pro]": {
            "enabled": true,
            "rules": {
                "variable.defaultLibrary": { // 標準ライブラリの変数や関数の色
                    "foreground": "#56b6c2"  // シアン系の色
                },
                "function.declaration": {    // 関数の定義部分の色
                    "foreground": "#61afef",   // 明るい青色
                    "italic": true             // 斜体にする
                },
                "parameter": {               // 関数の引数の色
                    "foreground": "#c678dd"  // 紫色
                },
                "type": {                    // 型ヒントの色
                    "foreground": "#e5c07b"  // 黄色
                }
            }
        }
    },
}
